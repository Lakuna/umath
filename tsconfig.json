// Up-to-date as of TypeScript version 5.0.4.
{
	// "files": [],
	// "extends": "",
	// "include": [],
	// "exclude": [],
	// "references": [],
	"compilerOptions": {
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"alwaysStrict": true,
		"exactOptionalPropertyTypes": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"useUnknownInCatchVariables": true,
		"allowUmdGlobalAccess": false,
		// "baseUrl": "",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		// "moduleSuffixes": [],
		"noResolve": false,
		// "paths": {},
		"resolveJsonModule": true,
		"resolvePackageJsonExports": true,
		"resolvePackageJsonImports": true,
		"rootDir": "src",
		// "rootDirs": [],
		// "typeRoots": [],
		// "types": [],
		"declaration": true,
		// "declarationDir": "",
		"declarationMap": true,
		"downlevelIteration": false,
		"emitBOM": false,
		"emitDeclarationOnly": false,
		"importHelpers": false,
		// "importsNotUsedAsValues": "error", // Deprecated
		"inlineSourceMap": false,
		"inlineSources": false,
		// "mapRoot": "",
		// "newLine": "crlf",
		"noEmit": false,
		"noEmitHelpers": false,
		"noEmitOnError": true,
		"outDir": "dist",
		// "outFile": "",
		"preserveConstEnums": true,
		// "preserveValueImports": false, // Deprecated
		"removeComments": true,
		"sourceMap": true,
		// "sourceRoot": "",
		"stripInternal": true,
		"verbatimModuleSyntax": true,
		"allowArbitraryExtensions": false,
		"allowImportingTsExtensions": false,
		"allowJs": false,
		"checkJs": false,
		"customConditions": [],
		"maxNodeModuleJsDepth": 0,
		"disableSizeLimit": false,
		"plugins": [],
		"allowSyntheticDefaultImports": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		// "isolatedModules": true, // Redundant with `verbatimModuleSyntax`.
		"preserveSymlinks": false,
		// "charset": "utf8", // Deprecated
		// "keyofStringsOnly": false, // Deprecated
		// "noImplicitUseStrict": false, // Deprecated
		"noStrictGenericChecks": false,
		// "out": "", // Deprecated
		"suppressExcessPropertyErrors": false,
		"suppressImplicitAnyIndexErrors": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"jsx": "preserve",
		"jsxFactory": "React.createElement",
		"jsxFragmentFactory": "React.Fragment",
		"jsxImportSource": "react",
		// "lib": [],
		"moduleDetection": "auto",
		"noLib": false,
		// "reactNamespace": "React", // Deprecated
		"target": "ESNext",
		"useDefineForClassFields": true,
		"diagnostics": false,
		"explainFiles": false,
		"extendedDiagnostics": false,
		"generateCpuProfile": "profile.cpuprofile",
		"listEmittedFiles": false,
		"listFiles": false,
		"traceResolution": false,
		"composite": true,
		"disableReferencedProjectLoad": false,
		"disableSolutionSearching": false,
		"disableSourceOfProjectReferenceRedirect": false,
		"incremental": true,
		"tsBuildInfoFile": ".tsbuildinfo",
		"noErrorTruncation": false,
		"preserveWatchOutput": false,
		"pretty": true,
		// "skipDefaultLibCheck": false, // Deprecated
		"skipLibCheck": false,
		"assumeChangesOnlyAffectDirectDependencies": false
	},
	"watchOptions": {
		"watchFile": "useFsEvents",
		"watchDirectory": "useFsEvents",
		"fallbackPolling": "dynamicpriority",
		"synchronousWatchDirectory": false,
		"excludeDirectories": [],
		"excludeFiles": []
	},
	"typeAcquisition": {
		"enable": true,
		"include": [],
		"exclude": [],
		"disableFilenameBasedTypeAcquisition": false
	}
}
